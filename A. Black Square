 
#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    char s[100001];
    cin>>s;
    int c1=0,c2=0,c3=0,c4=0;
    for(int i=0;i<strlen(s);i++){
        if(s[i]=='1')
            c1++;
         else if(s[i]=='2')
            c2++;
         else if(s[i]=='3')
            c3++;
         else
            c4++;
    }
    int total=c1*a+c2*b+c3*c+c4*d;
    cout<<total<<endl;
    return 0;
}
