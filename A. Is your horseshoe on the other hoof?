#include <bits/stdc++.h>
using namespace std;

int main()
{
    long a[5],c=0;
    for(int i=0; i<4; i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<4;i++){
        for(int j=0;j<i;j++){
            if(a[i]==a[j])
               {
                   a[j]=0;
                   c++;
               }
        }
    }
    cout<<c<<endl;
}
