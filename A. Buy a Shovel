#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n,r;
    cin>>n>>r;
    for(int i=1; ; i++)
    {
        int t=n*i;
        int m=t%10;

        if(m==r)
        {
            cout<<i<<endl;
            break;
        }
        if(m==0)
        {
            cout<<i<<endl;
            break;
        }
    }
}
