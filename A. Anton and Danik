#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int n,a=0,d=0;
    cin>>n;
    char w[n];
    for(int i=0; i<n; i++)
        cin>>w[i];
    for(int i=0; i<n; i++)
    {
        if(w[i]=='A')
            a++;
        else
            d++;
    }
    if(a>d)
        cout<<"Anton\n";
    else if(a==d)
        cout<<"Friendship\n";
    else
        cout<<"Danik\n";
}
